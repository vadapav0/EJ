------------INEX.JSP-----------
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Form Submission Example</title>
        <style>
            body {
                font-family: Arial, sans-serif;
            }
            form {
                margin: 20px 0;
            }
            input[type="text"] {
                width: 250px;
                padding: 5px;
            }
            label {
                display: block;
                margin-top: 10px;
            }
            input[type="button"] {
                margin-top: 10px;
                padding: 5px 10px;
            }   
        </style>
    </head>
    <body>
        <h1>Form Submission</h1>
        <form action="yourProcessingServlet.jsp" method="post">
            <label for="name">Enter your name:</label>
            <input type="text" id="name" name="name"><br>
            
            <label for="email">Enter your email id:</label>
            <input type="text" id="email" name="email"><br>
            
            <a href="implicitObjectEx.jsp?name=Tauseef"><input type="button" value="Button"></a>
        </form>
    </body>
</html>

------------implicitObjectEx-----------------
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
